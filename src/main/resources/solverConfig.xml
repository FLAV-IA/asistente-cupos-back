<?xml version="1.0" encoding="UTF-8"?>
<solver>
    <solutionClass>com.edu.asistente_cupos.service.sugerencia.opta.model.SugeridorComisionesSolution</solutionClass>
    <entityClass>com.edu.asistente_cupos.service.sugerencia.opta.model.PeticionAceptableDTO</entityClass>
    <constraintProviderClass>com.edu.asistente_cupos.service.sugerencia.opta.reglas.GeneracionSugerenciasConstraintProvider
    </constraintProviderClass>

    <termination>
        <secondsSpentLimit>10</secondsSpentLimit> <!--tiempo en la que tardara en resolver el problema-->
